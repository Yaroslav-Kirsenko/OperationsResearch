<Window x:Class="OperationsResearch.Dual.ShowSamle"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OperationsResearch.Dual"
        mc:Ignorable="d"
        Title="ShowSamle"
        Height="450" Width="850"
        WindowStartupLocation="CenterScreen"
        ResizeMode="CanResizeWithGrip">
    <Grid Margin="0,0,0,0"  Background="#FFD9FFFE">
        <Button Content="Повернутись назад" HorizontalAlignment="Left" Margin="2,7,0,0" VerticalAlignment="Top" Width="113" Height="22" Click="Button_Click_Exit" Background="#FFABFFEE"/>

        <!-- StackPanel для отображения значений меток -->
        <StackPanel x:Name="LabelsContainer" Margin="10,48,10,0" VerticalAlignment="Top" Height="334">
            <!-- Здесь будут отображаться добавленные Label -->
        </StackPanel>

    </Grid>
</Window>
