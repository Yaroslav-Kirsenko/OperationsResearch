<Window x:Class="OperationsResearch.Dual.ShowSamle"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OperationsResearch.Dual"
        mc:Ignorable="d"
        Title="ShowSamle"
        Height="450" Width="850"
        WindowStartupLocation="CenterScreen"
        ResizeMode="CanResizeWithGrip">
    <Grid Margin="0,0,0,0"  Background="#FFD9FFFE">
        <Button Content="Повернутись назад" HorizontalAlignment="Left" Margin="2,7,0,0" Width="113" Height="22" Click="Button_Click_Exit" Background="#FFABFFEE"/>

        <Button Content="Далі" HorizontalAlignment="Center" Margin="0,367,0,0" VerticalAlignment="Top" Width="830" Height="40" Click="Button_Click_Next" Background="#FFABFFEE"/>

        <StackPanel x:Name="ResultContainer" Orientation="Vertical">

        </StackPanel>

    </Grid>
</Window>
